{
  "name": "dotql",
  "description": "graphql like but query by plain json. No more DSL or AST parsing",
  "version": "0.8.4",
  "license": "MIT",
  "main": "index.js",
  "module": "module.js",
  "browser": "build/module.js",
  "scripts": {
    "clean": "del build",
    "build": "npm run clean && babel . --ignore 'test','**/*.test.js' --out-dir build",
    "dev": "babel . --ignore 'test','**/*.test.js' --out-dir build --watch",
    "prepublishOnly": "npm run build && npm test",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "esm": "^3.2.22",
    "eventemitter3": "3.1.0",
    "fast-stable-stringify": "1.0.0",
    "lodash": "^4.17.11",
    "lodash-es": "4.17.11",
    "p-is-promise": "2.1.0",
    "regenerator-runtime": "0.13.2",
    "shallow-equal": "1.1.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "delay": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-react-app": "4.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "3.6.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-react": "7.12.4",
    "jest": "24.7.1",
    "react": "16.8.6"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "graphql",
    "graphql-js",
    "json",
    "query",
    "cache"
  ],
  "size-limit": [
    {
      "path": "module.js",
      "limit": "500 ms"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotql/dotql.git"
  },
  "bugs": {
    "url": "https://github.com/ericfong/dotql/issues"
  },
  "homepage": "https://github.com/ericfong/dotql#readme",
  "author": "Eric Fong"
}
